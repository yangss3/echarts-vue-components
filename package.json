{
  "name": "@yangss/echarts-vue-components",
  "version": "1.0.7",
  "description": "A few simple chart components based on Echarts and Vue",
  "main": "lib/echarts-vue-components.ssr.js",
  "unpkg": "lib/echarts-vue-components.min.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/app.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "color": "^3.1.2",
    "echarts-liquidfill": "^2.0.5",
    "lodash": "^4.17.15",
    "vuepress": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "cross-env": "^7.0.2",
    "echarts": "^4.7.0",
    "minimist": "^1.2.5",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "echarts": "^4.7.0"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "echarts",
    "vue",
    "components"
  ],
  "author": {
    "name": "Nicholas Yang",
    "email": "nicholasy333@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yangss3/echarts-vue-components.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://yangss3.github.io/echarts-vue-components"
}
